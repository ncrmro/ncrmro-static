{
  "name": "react-static-example-typescript",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000 -s",
    "prettier": "prettier --write 'src/**/*{.ts,.tsx,js}'",
    "sentry_release": "sentry-cli releases new --finalize -p ncrmro-static $COMMIT_REF && sentry-cli releases set-commits --commit ncrmro/ncrmro-static@$COMMIT_REF $COMMIT_REF && sentry-cli releases -p ncrmro-static files $COMMIT_REF upload-sourcemaps dist && sentry-cli releases deploys $COMMIT_REF new -e $ENVIRONMENT"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@sentry/browser": "^4.6.4",
    "axios": "^0.18.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.3.12",
    "react-markdown": "^4.0.6",
    "react-static": "^6.0.18"
  },
  "devDependencies": {
    "@types/node": "^10.12.23",
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack-env": "^1.13.6",
    "dotenv": "^6.2.0",
    "@sentry/cli": "^1.40.0",
    "prettier": "1.16.4",
    "react-static-plugin-typescript": "^3.1.0",
    "serve": "10.1.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
